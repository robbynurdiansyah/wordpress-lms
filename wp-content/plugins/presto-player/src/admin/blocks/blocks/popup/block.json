{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "presto-player/popup",
  "apiVersion": 2,
  "title": "Presto Popup",
  "category": "presto",
  "keywords": ["popup", "lightbox", "modal"],
  "description": "Easily open your Presto Player videos in a Popup.",
  "textdomain": "presto-player",
  "supports": {
    "interactivity": true
  },
  "viewScriptModule": "presto-player-popup",
  "editorStyle": "file:./editor.css",
  "attributes": {
    "popupId": {
      "type": "string",
      "default": ""
    }
  },
  "providesContext": {
    "presto-player/popup-id": "popupId"
  }
}
